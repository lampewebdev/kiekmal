<!DOCTYPE html>
<html>
<head>
  <title>Kmwi</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
  Welcome <%= current_user.name %>!
  <%= link_to "Sign Out", signout_path %>
<% else %>
  <%= link_to "Sign in with facebook", "/auth/facebook" %>
<% end %>
<%= yield %>

</body>
</html>
